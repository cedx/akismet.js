{
  "metadata": {
    "header": {"background": "#f9a825", "color": "white"},
    "title": "Akismet for JS"
  },
  "opts": {
    "destination": "doc/api",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/tidy-jsdoc"
  },
  "plugins": ["plugins/markdown"],
  "source": {"include": ["lib"]},
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  }
}
